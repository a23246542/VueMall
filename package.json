{
  "name": "vuemall",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "core-js": "^3.6.5",
    "jquery": "^3.5.1",
    "lodash": "^4.17.19",
    "popper": "^1.0.1",
    "popper.js": "^1.16.1",
    "quill": "^1.3.7",
    "sweetalert2": "^10.0.2",
    "swiper": "^6.2.0",
    "vee-validate": "^3.3.9",
    "vue": "^2.6.11",
    "vue-awesome-swiper": "^4.1.1",
    "vue-axios": "^2.1.5",
    "vue-debounce": "^2.5.7",
    "vue-loading-overlay": "^3.3.2",
    "vue-router": "^3.3.4",
    "vue-sweetalert2": "^3.0.6",
    "vue2-editor": "^2.10.2",
    "vuex": "^3.5.1",
    "mockjs": "^1.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "^4.5.4",
    "@vue/cli-service": "~5.0.8",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.8.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "globals": {
      "$": true,
      "jquery": true
    },
    "rules": {
      "indent": [
        "error",
        2,
        {
          "SwitchCase":1
        }
      ],
      "max-len": [
        1,
        100,
        2,
        {
          "ignoreComments": true,
          "ignoreUrls": true,
          "ignoreStrings": true
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "globals": {
    "bootstrap": "readonly"
  }
}
